@model IEnumerable<TMDTProject.Models.VeMayBay>
@{
    Layout = "~/Views/SharedAdmin/AdminSharedLayout.cshtml";
}
<!-- Begin Page Content -->
<div class="container-fluid">

    <!-- Page Heading -->
    <h1 class="h3 mb-2 text-gray-800">Danh sách dịch vụ của đối tác</h1>
    <a href="/Admin/ThemVeMayBay" class="btn btn-success btn-circle">
            <i class="fas fa-plus"></i>
        </a>
    @*<a href="#" class="btn btn-danger btn-circle">
            <i class="fas fa-trash"></i>
        </a>*@
    <!-- DataTales Example -->
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">Danh Sách Dịch Vụ Của Đối Tác</h6>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table id="table_doitac" class="table table-striped dataTable">
                    <colgroup>
                        <col style="width: 6%">
                        <col style="width: 10%">
                        <col style="width: 10%">
                        <col style="width: 30%">
                        <col style="width: 10%">
                        <col style="width: 20%">
                        <col style="width: 10%">
                    </colgroup>
                    <thead>
                        <tr>
                            <th>STT</th>
                            <th>Tên đối tác</th>
                            <th>Loại Dịch Vụ</th>
                            <th>Tên Dịch Vụ</th>
                            <th>Giá</th>
                            <th>Chi Tiết</th>
                            <th>Tuỳ Chỉnh</th>
                        </tr>
                    </thead>
                    <tbody>
                        @{
                            int stt = 0;
                            foreach (TMDTProject.Models.VeMayBay veMayBay in Model)
                            {
                                stt++;
                                <tr>
                                    <td>@stt</td>
                                    <td>@veMayBay.DoiTac.TenDT</td>
                                    <td>@veMayBay.DoiTac.LoaiDichVu.TenLoaiDV</td>
                                    <td>@veMayBay.TenVe</td>
                                    <td>@veMayBay.Gia</td>
                                    <td>@veMayBay.ChiTiet</td>
                                    <td><a href="/Admin/SuaVeMayBay/@veMayBay.MaVeMB">Sửa</a>&nbsp;|&nbsp;<a href="/Admin/XoaVeMayBay/@veMayBay.MaVeMB" )">Xóa</a></td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>

</div>
